@model CustomIdentityApp.Models.AppUser

<div class="bg-primary m-1 p-1"><h4>Edit user</h4></div>
<div asp-validation-summary="All" class="text-danger"></div>
<form asp-action="Edit" method="post">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" class="form-control" disabled />
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input name="password" class="form-control"/>
    </div>
    <button type="submit" asp-route-id="@Model.Id" class="btn btn-primary">Save</button>
    <a asp-action="Index" asp-controller="ImageGallery" class="btn btn-primary">Return</a>
</form>